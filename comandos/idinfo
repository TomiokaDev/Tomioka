const Discord = require('discord.js');
const config = require('../config.js');
const moment = require("moment");

module.exports = async(client, message, args) => {

try{
let id = await client.users.fetch();

if(!id) {
 return message.channel.send("Ingresa una id")
}
  const embed = new Discord.MessageEmbed()
 .addField("Username", id.tag)
 .addField("ID", id.id)
 .addField("Estado", id.presence.status)
 .addField("Fecha de creaci√≥n", id.createdAt)
 .addField("Bot?", id.bot)
 .setThumbnail(id.avatarURL({ dynamic: true, format: 'png', size: 1024 }))
 .setFooter(`Ejecutado por: ${message.author.tag}`, message.author.avatarURL())
 .setColor(config.color)
 message.channel.send({ embed: embed })
} catch (err) {
    console.log(err);
    return message.reply("Hubo un error");
}
};
module.exports.config = {
command:"idinfo",
aliases:["idinfo"],
cooldown: 5
}
