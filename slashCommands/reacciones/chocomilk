const Discord = require('discord.js');
const { EmbedBuilder } = require('discord.js');
const config = require('../../config.json');

module.exports = {
	name: 'chocomilk',
	description: 'Comando para darle tu chocomilk a alguien',
	guildOnly: true,
	cooldown: 5,
	run: async (client, interaction) => {

  let persona = message.mentions.users.first()                                                    //tk!punch
    const embednopersona = new Discord.EmbedBuilder() 
    .setTitle("Creo que es medio egoista no compartir tu chocomilk >:(")
    .setColor(config.color)
    .setImage("https://cdn.discordapp.com/attachments/671170382010515466/733021319679049779/descargar.jpg")
    .setFooter({text: `Ejecutado por: ${interaction.member.user.tag}`, iconURL: interaction.member.user.avatarURL()})
        if(!persona) return interaction.reply({ embed: embednopersona })
    const embedpersona = new Discord.EmbedBuilder() 
    .setDescription(`**${interaction.member.displayName}** le dio chocomilk a ` + `**`+persona.username+`**`)
    .setColor(config.color)
    .setImage("https://cdn.discordapp.com/attachments/710648492791431238/711378070786932826/ENdaoP_WoAEs7WH.jpg")
    .setFooter({text: `Ejecutado por: ${interaction.member.user.tag}`, iconURL: interaction.member.user.avatarURL()})
        interaction.reply({ embed: embedpersona })

    },
  };
